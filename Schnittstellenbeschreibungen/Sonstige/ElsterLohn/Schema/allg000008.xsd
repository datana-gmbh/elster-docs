	<xs:simpleType name="allg_DatumMelderechtSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Datum 8-stellig ohne Trennzeichen</xs:documentation>
			<xs:documentation xml:lang="DE">Format : JJJJMMTT</xs:documentation>
			<xs:documentation xml:lang="DE">00000000 für melderechtlich unbekannt</xs:documentation>
			<xs:documentation xml:lang="DE">JJJJ0000 oder JJJJMM00 für melderechtlich teilbekannt</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringSType">
			<xs:length value="8"/>
			<xs:pattern value="((((18)(8|9)\d)|((19|20)\d{2}))(((([0][1-9])|([1][0-2]))[0-3][0-9])|(0000)))|(00000000)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_DatumSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Datum und Uhrzeit 14-stellig ohne Trennzeichen</xs:documentation>
			<xs:documentation xml:lang="DE">Format : JJJJMMTThhmmss</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringSType">
			<xs:length value="14"/>
			<xs:pattern value="\d{14}"/>
