					<xs:documentation xml:lang="DE">Staat / Gebiet             =Afghanistan</xs:documentation>
					<xs:documentation xml:lang="DE">Staatsangehörigkeit   =afghanisch</xs:documentation>
					<xs:documentation xml:lang="DE">Länderschlüssel        =423</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="ET.*">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Staat / Gebiet             =Ägypten</xs:documentation>
					<xs:documentation xml:lang="DE">Staatsangehörigkeit   =ägyptisch</xs:documentation>
					<xs:documentation xml:lang="DE">Länderschlüssel        =287</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="AL.*">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Staat / Gebiet             =Albanien</xs:documentation>
