					<xs:documentation xml:lang="DE">Länderschlüssel        =121</xs:documentation>
				</xs:annotation>
			</xs:pattern>
			<xs:pattern value="DZ.*">
				<xs:annotation>
					<xs:documentation xml:lang="DE">Staat / Gebiet             =Algerien</xs:documentation>
					<xs:documentation xml:lang="DE">Staatsangehörigkeit   =algerisch</xs:documentation>
					<xs:documentation xml:lang="DE">Länderschlüssel        =221</xs:documentation>
