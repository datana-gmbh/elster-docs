			<xs:documentation xml:lang="DE">04 = Bremen</xs:documentation>
			<xs:documentation xml:lang="DE">02 = Hamburg</xs:documentation>
			<xs:documentation xml:lang="DE">06 = Hessen</xs:documentation>
			<xs:documentation xml:lang="DE">13 = Mecklenburg-Vorpommern</xs:documentation>
			<xs:documentation xml:lang="DE">03 = Niedersachsen</xs:documentation>
