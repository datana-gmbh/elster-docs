	<!-- B -->
	<xs:simpleType name="allg_BaseStringSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">die Einschränkungen sind in der Schnittstellenbeschreibung SST_Arbeitgeber zu entnehmen"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value="[\p{L}\p{P}\p{Zs}\p{Nd}]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_BaseStringLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Der String muss mindestens 1 Zeichen vom Typ {N} oder {L} beinhalten, zusätzlich gelten die Einschränkungen  des allg_BaseStringNoLeadingOrTrailingSeparators</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value=".*([\p{N}]|[\p{L}]).*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_BaseStringNoLeadingOrTrailingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">das letzte Zeichen darf nicht vom Typ {Z} oder ^ sein, zusätzlich gelten die Einschränkungen  des allg_BaseStringNoLeadingSeparators</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringNoLeadingSeparators">
			<xs:pattern value=".*[\P{Z}]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_BaseStringNoLeadingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">das erste Zeichen darf nicht vom Typ {Z} oder ^ sein, zusätzlich gelten die Einschränkungen  des allg_BaseStringUltraLightSType</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:allg_BaseStringUltraLightSType">
			<xs:pattern value="[\P{Z}].*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_BaseStringUltraLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">alle Zeichen die nicht Kategorien C oder Zl gehören </xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterbase:datatypeC">
			<xs:pattern value="([^\n\r\t\p{C}\p{Zl}])*"/>
		</xs:restriction>
	</xs:simpleType>
