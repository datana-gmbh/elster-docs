			<xs:pattern value="[A-Z]{8}\d{2}[A-L]{1}[0][1-9][A-Z]{1}"/>
			<xs:pattern value="[A-Z]{8}\d{2}[A-L]{1}[1-2][0-9][A-Z]{1}"/>
