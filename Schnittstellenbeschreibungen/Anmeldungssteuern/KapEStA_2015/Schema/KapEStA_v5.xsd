					<xs:documentation>Kirchensteuer zur Kapitalertragsteuer für Korrekturzwecke für Zeiträume vor 2015!</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="KorKiEv" type="Betrag15stellig" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Korrekturbetrag Evangelische Kirchensteuer</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="KorKiRK" type="Betrag15stellig" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Korrekturbetrag Römisch-Katholische Kirchensteuer</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="KorKuJKOKH" type="Betrag15stellig" minOccurs="0">
