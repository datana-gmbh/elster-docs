			<xs:element name="KapESch15Sz" type="Betrag15stellig" minOccurs="0"/>
			<xs:element name="KapWertSchSt" type="GanzerBetrag12stellig" minOccurs="0"/>
			<xs:element name="KapWertSchSz" type="Betrag15stellig" minOccurs="0"/>
