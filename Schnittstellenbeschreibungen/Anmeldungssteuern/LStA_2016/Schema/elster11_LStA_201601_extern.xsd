<?xml version="1.0" encoding="ISO-8859-15"?><!-- Version 2018.1 --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:elster="http://www.elster.de/elsterxml/schema/v11" targetNamespace="http://www.elster.de/elsterxml/schema/v11" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1">	<xs:include schemaLocation="../../../ElsterBasisSchema/Schema/th000011_extern.xsd"/>	<xs:include schemaLocation="../../../ElsterBasisSchema/Schema/ndh000011.xsd"/>	<xs:include schemaLocation="./lsta_201601.xsd">		<xs:annotation>			<xs:documentation xml:lang="DE">Lohnsteueranmeldung</xs:documentation>		</xs:annotation>	</xs:include>	<!-- RootElement = Elster _______________________________________________________________-->	<xs:element name="Elster">		<xs:complexType>			<xs:sequence>				<xs:element ref="elster:TransferHeader"/>				<xs:element name="DatenTeil" type="elster:DatenTeilCType">					<xs:annotation>						<xs:documentation xml:lang="DE">verschlüsselt</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>		<!-- aus Performance Gründen nur in den externen Schemata zu verwenden		<xs:unique name="EindeutigesNutzdatenTicket">			<xs:selector xpath="elster:DatenTeil/elster:Nutzdatenblock/elster:NutzdatenHeader"/>			<xs:field xpath="elster:NutzdatenTicket"/>		</xs:unique>		-->	</xs:element>	<!-- Definierung einfacher Elements________________________________________________________-->	<!-- Definierung globaler complex Types __________________________________________________ -->	<xs:complexType name="DatenTeilCType">		<xs:sequence>			<xs:element name="Nutzdatenblock" type="elster:NutzdatenblockCType" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="NutzdatenblockCType">		<xs:annotation>			<xs:documentation xml:lang="DE">Der Nutzdatenblock enthält jeweils einen "NutzdatenHeader"- und einen "Nutzdaten"-Tag</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="NutzdatenHeader" type="elster:NutzdatenHeaderCType"/>			<xs:element name="Nutzdaten" type="elster:NutzdatenCType">				<!--		aus Performancegründen nur in den externen Schemata's zu verwenden				<xs:unique name="EindeutigesOrdnungsmerkmal">					<xs:selector xpath="elster:Lohnsteuerbescheinigung/elster:Allgemein"/>					<xs:field xpath="elster:Ordnungsmerkmal"/>				</xs:unique>-->			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="NutzdatenCType">		<xs:sequence>			<xs:element name="Anmeldungssteuern" type="elster:AnmeldungssteuernCType"/>		</xs:sequence>	</xs:complexType>	<!-- Definierung globaler Attribute _______________________________________________________ --></xs:schema>