										</xs:sequence>
										<xs:element name="GKPLZ" type="AnschriftDeutschePLZType"/>
									</xs:choice>
								</xs:sequence>
							</xs:sequence>
							<xs:sequence>
								<xs:element name="PostfachOrt" type="AnschriftOrtType"/>
